{
  "name": "angular-datatables-systemjs-example",
  "version": "2.0.0-dev",
  "description": "Example for using Angular directive for DataTables",
  "scripts": {
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "typings install",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update",
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "keywords": [
    "Angular",
    "2",
    "DataTables",
    "Example"
  ],
  "author": "Louis LIN <louis.lin.87@gmail.com> (https://l-lin.github.io/)",
  "contributors": [
    "Michael Bennett <michael@strukt.org>"
  ],
  "license": "MIT",
  "dependencies": {
    "@angular/common": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/core": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/router": "3.0.2",
    "@angular/upgrade": "2.0.2",
    "angular-datatables": "file:../..",
    "angular2-in-memory-web-api": "0.0.20",

    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "datatables.net": "^1.10.12",
    "datatables.net-bs": "^1.10.12",
    "datatables.net-dt": "^1.10.12",
    "datatables.net-responsive": "^2.1.0",
    "datatables.net-responsive-bs": "^2.1.0",
    "datatables.net-scroller": "^1.4.2",
    "datatables.net-scroller-bs": "^1.4.2",
    "jquery": "^3.1.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "uglify-js": "^2.7.3",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "lite-server": "^2.2.0",
    "typescript": "^2.0.2",
    "typings": "^1.0.4",
    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "tslint": "^3.7.4",
    "lodash": "^4.11.1",
    "jasmine-core": "~2.4.1",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l-lin/angular-datatables.git"
  },
  "bugs": {
